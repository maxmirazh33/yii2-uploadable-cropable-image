{
    "name": "maxmirazh33/yii2-uploadable-cropable-image",
    "description": "Yii2 extension for upload and crop images",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "extension",
        "jcrop",
        "image",
        "image upload",
        "image crop"
    ],
    "homepage": "https://github.com/maxmirazh33/yii2-uploadable-cropable-image",
    "license": "MIT",
    "authors": [
        {
            "name": "Maxim Kolokolnikov",
            "email": "maxmirazh33@gmail.com",
            "homepage": "http://maximkolokolnikov.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/maxmirazh33/yii2-uploadable-cropable-image/issues",
        "source": "https://github.com/maxmirazh33/yii2-uploadable-cropable-image"
    },
    "require": {
        "yiisoft/yii2": "2.0.*",
        "yiisoft/yii2-imagine": "2.*",
        "yiisoft/yii2-bootstrap": "2.*",
        "bower-asset/Jcrop": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "maxmirazh33\\image\\": "src"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
